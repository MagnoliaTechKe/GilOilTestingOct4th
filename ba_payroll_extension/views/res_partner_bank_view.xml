<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Inherit Form View to Modify it -->
    <record id="res_bank_account_view_inherit_print_bank_details" model="ir.ui.view">
        <field name="name">Res Bank Account View Inherit Print Bank Details</field>
        <field name="model">res.partner.bank</field>
        <field name="inherit_id" ref="account.view_partner_bank_form_inherit_account"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='acc_holder_name']" position="before">
                <field name="bank_branch"/>
            </xpath>

        </field>
    </record>
</odoo>